<div class="container">
  <h1><%= @cocktail.name%></h1>
  <%= simple_form_for [@cocktail, @dose] do |f| %>
  <%= f.input :description %>
  <%= f.association :ingredient %>
  <%= f.submit "Add doses", class: "btn btn-primary" %>
<% end %>

  <%= link_to "Back to My Cocktails", cocktails_path %>
</div>
